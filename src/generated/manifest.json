{
  "store": "retail",
  "version": "v0.0.1",
  "corex": {
    "version": "v0.0.1",
    "dependencies": {
      "corex-store-interface": "^0.1.1",
      "header-simple": "^0.1.1"
    }
  },
  "dependencies": {},
  "interfaces": {
    "header": "header-simple"
  },
  "routes": [
    {
      "path": "/carrito",
      "component": "Account",
      "content": false
    },
    {
      "path": "/busqueda/:query?",
      "component": "Search",
      "content": false
    },
    {
      "path": "/:category/:family/:line/:productName/:productID",
      "component": "PDP",
      "content": false
    },
    {
      "component": "Content",
      "content": true
    }
  ]
}
